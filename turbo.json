{
	"$schema": "https://v2-8-12.turborepo.dev/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".next/**", "!.next/cache/**"]
		},
		"dev": {
			"persistent": true,
			"cache": false
		},
		"start": {
			"persistent": true,
			"cache": false
		},
		"clean": {
			"cache": false
		},
		"//#clean": {
			"cache": false
		},
		"db:push": {
			"cache": false
		},
		"db:studio": {
			"cache": false,
			"persistent": true
		}
	},
	"globalEnv": ["NODE_ENV"],
	"globalPassThroughEnv": ["DATABASE_URL"]
}
